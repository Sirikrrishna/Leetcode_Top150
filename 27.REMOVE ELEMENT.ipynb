    "### 27.REMOVE ELEMENT"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "559331a3",
   "metadata": {},
   "outputs": [],
   "source": [
    "class Solution:\n",
    "    def removeElement(self, nums: List[int], val: int) -> int:\n",
    "        while val in nums:\n",
    "            nums.remove(val)\n",
    "        return len(nums)"
   ]
  }
